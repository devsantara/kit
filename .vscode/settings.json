{
  "typescript.tsdk": "node_modules/typescript/lib",
  // https://eslint.style/guide/faq#vs-code
  "editor.formatOnSave": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
   "eslint.rules.customizations": [
    {
      "rule": "@stylistic/*",
      "fixable": true,
      "severity": "off"
    }
  ]
}
