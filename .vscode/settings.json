{
  // Editor
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "prettier.prettier-vscode",

  // Extension - Prettier
  "prettier.enable": true,
  "prettier.configPath": "prettier.config.js",

  // Extension - ESLint
  "eslint.enable": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  }
}
